import http.server\
import socketserver\
import json\
import os\
from urllib.parse import parse_qs\
\
PORT = 8000\
\
class FootballHandler(http.server.SimpleHTTPRequestHandler):\
    def do_GET(self):\
        if self.path == '/':\
            self.path = '/index.html'\
        return http.server.SimpleHTTPRequestHandler.do_GET(self)\
    \
    def do_POST(self):\
        if self.path == '/save_training':\
            content_length = int(self.headers['Content-Length'])\
            post_data = self.rfile.read(content_length)\
            data = parse_qs(post_data.decode())\
            \
            # Sauvegarder les donn??es\
            self.save_data('entrainement', {\
                'type': data.get('type', [''])[0],\
                'duree': data.get('duree', [''])[0],\
                'intensite': data.get('intensite', [''])[0],\
                'douleur_genou': 'douleur_genou' in data,\
                'notes': data.get('notes', [''])[0]\
            })\
            \
            self.send_response(200)\
            self.send_header('Content-type', 'application/json')\
            self.end_headers()\
            self.wfile.write(json.dumps({'status': 'success'}).encode())\
        \
        else:\
            self.send_error(404)\
    \
    def save_data(self, data_type, data):\
        os.makedirs('data', exist_ok=True)\
        filename = f'data/{data_type}.json'\
        \
        # Charger les donn??es existantes\
        if os.path.exists(filename):\
            with open(filename, 'r') as f:\
                existing_data = json.load(f)\
        else:\
            existing_data = []\
        \
        # Ajouter les nouvelles donn??es\
        existing_data.append(data)\
        \
        # Sauvegarder\
        with open(filename, 'w') as f:\
            json.dump(existing_data, f, indent=2)\
\
# Cr??er la page HTML\
if not os.path.exists('index.html'):\
    with open('web_manager.py', 'r') as source:\
        html_content = []\
        in_html = False\
        for line in source:\
            if 'HTML_PAGE = """' in line:\
                in_html = True\
                continue\
            if in_html:\
                if '"""' in line:\
                    break\
                html_content.append(line)\
        \
        with open('index.html', 'w') as f:\
            f.writelines(html_content)\
\
print(f"???? Serveur Football Manager d??marr??!")\
print(f"???? Ouvrez votre navigateur et allez sur: http://localhost:{PORT}")\
print("???? Accessible depuis votre t??l??phone sur le m??me r??seau WiFi")\
print("???? Appuyez sur Ctrl+C pour arr??ter le serveur")\
\
with socketserver.TCPServer(("", PORT), FootballHandler) as httpd:\
    httpd.serve_forever()\
